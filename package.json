{
  "name": "jira-connector-http",
  "version": "0.1.0",
  "description": "JIRA Web Source connector for Reporting tools such as Power BI and Excel",
  "author": "Pierre Awaragi <pierre@awaragi.com>",
  "private": false,
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "format": "prettier --write \"*.js\" \"*.json\" \"src/**/*.js\" \"test/**/*.js\"",
